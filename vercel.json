{
  "headers": [
    {
      "source": "/assets/(.*\\.(?:jpg|jpeg|png|gif|svg|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
